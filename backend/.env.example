# Google Gemini API Key (used by scripts/train_vanna.py)
GEMINI_API_KEY=your_gemini_api_key_here

# LLM Model Names
GROQ_MODEL=llama-3.3-70b-versatile
GEMINI_MODEL=gemini-2.0-flash
